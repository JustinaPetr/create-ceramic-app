{
  "name": "@ceramicnetwork/create-ceramic-app",
  "version": "2.0.0",
  "description": "Create an example app with Ceramic and ComposeDB",
  "author": "3Box Labs",
  "bin": {
    "create-ceramic-app": "./bin/run.js"
  },
  "dependencies": {
    "@clack/core": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4",
    "picocolors": "^1.0.0",
    "tiged": "^2.12.5"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.52.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10",
    "oclif": "^3.17.2",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ceramicstudio/create-ceramic-app",
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "create-ceramic-app",
    "default": "clone",
    "dirname": "create-ceramic-app",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "clone": {
        "description": "Create a sample app with Ceramic and ComposeDB"
      }
    }
  },
  "repository": "git+https://github.com/ceramicstudio/create-ceramic-app.git",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "bugs": "https://github.com/ceramicstudio/create-ceramic-app/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
